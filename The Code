#include <iostream>
using namespace std;

int main() {
    int aquariumDavid = 8;
    int aquariumAlex = 11;

    int temp;

    temp = aquariumDavid;
    aquariumDavid = aquariumAlex;
    aquariumAlex = temp;

    
    cout << "David's aquarium: " << aquariumDavid << endl;
    cout << "Alex's aquarium: " << aquariumAlex;
    return 0;
}
